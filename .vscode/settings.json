{
  "cSpell.words": [
    "activitydefinition",
    "argfile",
    "cdshooks",
    "clinicalreasoning",
    "Codeable",
    "Configurer",
    "CONTINUOUSVARIABLE",
    "CRMI",
    "debuggable",
    "DENOMINATOREXCEPTION",
    "DENOMINATOREXCLUSION",
    "dstu",
    "Fhir",
    "fhirpath",
    "Gson",
    "INITIALPOPULATION",
    "Javadoc",
    "JXSON",
    "MEASUREOBSERVATION",
    "MEASUREPOPULATION",
    "MEASUREPOPULATIONEXCLUSION",
    "measurereport",
    "mgpc",
    "mgsc",
    "mrgc",
    "mrgsc",
    "msdc",
    "mvnw",
    "numer",
    "NUMERATOREXCLUSION",
    "opencds",
    "PATIENTLIST",
    "plandefinition",
    "ppca",
    "proxying",
    "qicore",
    "questionnaireresponse",
    "sdes",
    "sgpc",
    "Sonarlint",
    "SUBJECTLIST",
    "testng",
    "Unvalidated",
    "uscore"
  ],
  "cSpell.enabledLanguageIds": [
    "java",
    "json",
    "xml",
    "markdown"
  ],
  "cSpell.ignorePaths": [
    ".git/objects",
    ".vscode",
    "**/pom.xml",
    "checkstyle.xml"
  ],
  "commandOnAllFiles.commands": {
    "Format Java File": {
      "command": "editor.action.formatDocument",
      "includeFileExtensions": [
        ".java"
      ],
      "excludeFolders": [
        "target",
        "generated-sources"
      ]
    },
    "Format XML Files": {
      "command": "editor.action.formatDocument",
      "includeFileExtensions": [
        ".xml"
      ],
      "excludeFolders": [
        "target",
        "generated-sources"
      ]
    },
    "Format JSON Files": {
      "command": "editor.action.formatDocument",
      "includeFileExtensions": [
        ".json"
      ],
      "excludeFolders": [
        "target",
        "generated-sources"
      ]
    },
    "Format Markdown Files": {
      "command": "editor.action.formatDocument",
      "includeFileExtensions": [
        ".md"
      ],
      "excludeFolders": [
        "target",
        "generated-sources"
      ]
    }
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.detectIndentation": false,
  "editor.foldingImportsByDefault": true,
  "editor.tabSize": 2,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.dependency.packagePresentation": "hierarchical",
  "java.jdt.ls.vmargs": "-noverify -Xmx16G -XX:+UseG1GC -XX:+UseStringDeduplication",
  "java.compile.nullAnalysis.mode": "automatic",
  "java.format.settings.url": ".vscode/config/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "[markdown]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "cSpell.fixSpellingWithRenameProvider": true,
    "cSpell.advanced.feature.useReferenceProviderWithRename": true,
    "cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/"
  },
  "markdownlint.config": {
    "MD007": {
      "indent": 4
    }
  }
}